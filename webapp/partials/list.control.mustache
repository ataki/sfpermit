<div class="head">
  <h2>Permits</h2>
  <span class="s-h3">Sort by</span>
  <span class="sort-by proximity text-primary">Proximity</span>
  <span class="sort-by risk text-primary">Risk</span>
  <span class="sort-by units text-primary">Net Units</span>
  <a class="close">&times;</a> 
</div>
<ul>
    {{#collection}}
    <li class="model-row" data-id="{{id}}">
        <a href="{{url}}">
            <strong>{{name}}</strong><br/>
            <span>{{days}} old</span>, 
            <span>{{net_units}} number of units</span>,
            <span>{{prediction}} chance of success</span>
        </a>
    </li>
    {{/collection}}
</ul>
